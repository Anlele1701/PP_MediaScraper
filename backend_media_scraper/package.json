{
  "name": "backend_media_scraper",
  "version": "1.0.0",
  "main": "dist/app.js",
  "scripts": {
    "start": "tsc && node dist/app.js",
    "dev": "ts-node src/app.ts",
    "build": "tsc",
    "seed": "ts-node src/seed.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint \"{src}/**/*.ts\" --fix",
    "lint:check": "eslint \"src/**/*.{js,ts}\"",
    "migration:create": "sh -c 'typeorm migration:create ./src/migrations/$1' --",
    "migration:run": "npm run build && typeorm migration:run -d ./dist/config/data-source.js",
    "migration:revert": "npm run build && typeorm migration:revert -d ./dist/config/data-source.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "axios": "^1.7.7",
    "cheerio": "^1.0.0-rc.12",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "pg": "^8.17.1",
    "reflect-metadata": "^0.2.2",
    "typeorm": "^0.3.28",
    "uuid": "^9.0.1",
    "winston": "^3.19.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.6",
    "@types/node": "^25.0.9",
    "eslint": "^9.39.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
